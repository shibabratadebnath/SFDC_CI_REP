A4J.AJAX.XMLHttpRequest.prototype._copyAttribute = function (src, dst, attr) {
    var value = src.getAttribute(attr);
    if (value) {
        try {
            dst.setAttribute(attr, value);
        } catch (err) {
            //alert('Error with Salesforce: ' + err.description + '\nattr: ' + attr + '\n');
        }
    }
};

